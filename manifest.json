{
    "manifest_version": 2,
    "name": "HTTPS Support Resource Checker",
    "description": "Generate Network Resource Report",
    "version": "1.0",
    "permissions": [ "http://*/", "https://*/" ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
    "devtools_page": "devtools-page.html",
    "background": {
        "scripts": ["jquery-2.1.0.min.js", "underscore-min.js", "devtools-background.js" ]
    }
}
